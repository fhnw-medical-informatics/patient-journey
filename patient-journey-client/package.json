{
  "name": "patient-journey-client",
  "version": "2.0.0",
  "private": true,
  "homepage": "/patient-journey",
  "scripts": {
    "build": "tsc && vite build",
    "build-storybook": "build-storybook",
    "coverage": "vitest run --coverage",
    "predeploy": "tsc && vite build --base=/pj/app/",
    "deploy": "./deploy.sh",
    "lint": "eslint --max-warnings 0 './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "prepare": "cd .. && husky install patient-journey-client/.husky",
    "prepreview": "yarn build",
    "preview": "vite preview",
    "start": "vite",
    "storybook": "start-storybook -p 6006",
    "test": "vitest",
    "test:ci": "CI=true vitest run"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/semi": [
            "error",
            "never"
          ]
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.4",
    "@mui/x-data-grid-pro": "^5.10.0",
    "@nivo/bar": "^0.79.1",
    "@nivo/core": "^0.79.0",
    "@reduxjs/toolkit": "^1.8.1",
    "d3-array": "^3.1.6",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "papaparse": "^5.3.2",
    "react": "18.1.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "18.1.0",
    "react-redux": "8.0.1",
    "react-split-pane": "0.1.92",
    "react-svg-timeline": "^0.23.3",
    "react-virtualized-auto-sizer": "^1.0.6",
    "tss-react": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/node-logger": "^6.4.22",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.22",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/d3-array": "^3.0.2",
    "@types/d3-scale": "^4.0.2",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "17.0.31",
    "@types/papaparse": "^5.3.2",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-redux": "7.1.24",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@vitejs/plugin-react": "^1.3.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.15.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^8.0.1",
    "jsdom": "latest",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "prettier-plugin-packagejson": "2.2.17",
    "storybook-builder-vite": "^0.1.23",
    "storybook-dark-mode": "^1.1.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vite-plugin-checker": "^0.4.6",
    "vitest": "^0.12.3",
    "whatwg-fetch": "^3.6.2"
  }
}
